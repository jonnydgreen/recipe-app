{
  "name": "recipe-book-api",
  "version": "0.0.1",
  "description": "The API For Recipe Book",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo .nyc_output && tsc -b --clean .",
    "compile": "tsc -b -v .",
    "build": "npm run clean && npm run -- compile",
    "postinstall": "prisma generate",
    "dev": "nodemon -q --exec \"prisma migrate deploy && node .\""
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/opentelemetry-cloud-trace-exporter": "1.2.0",
    "@google-cloud/opentelemetry-cloud-trace-propagator": "0.15.0",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/instrumentation": "0.35.1",
    "@opentelemetry/resources": "1.9.1",
    "@opentelemetry/sdk-trace-base": "1.9.1",
    "@opentelemetry/sdk-trace-node": "1.9.1",
    "@opentelemetry/semantic-conventions": "1.9.1",
    "@prisma/client": "4.9.0",
    "fastify": "4.12.0",
    "graphql": "16.6.0",
    "inversify": "6.0.1",
    "mercurius": "12.0.0",
    "pino": "8.8.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "nodemon": "2.0.20",
    "prisma": "4.9.0",
    "sinon": "15.0.1",
    "tap": "16.3.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "^18"
  },
  "keywords": [],
